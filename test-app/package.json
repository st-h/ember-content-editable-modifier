{
  "name": "test-app",
  "version": "3.0.0",
  "description": "An ember modifier that enables contenteditable fields",
  "keywords": [
    "content-editable",
    "contenteditable",
    "modifier"
  ],
  "repository": "https://github.com/st-h/ember-content-editable-modifier",
  "license": "MIT",
  "author": "st-h",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"yarn:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"yarn:lint:css -- --fix\"",
    "lint:fix": "concurrently \"yarn:lint:*:fix\" --names \"fix:\" --prefixColors auto && yarn format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"yarn:lint\" \"yarn:test:*\" --names \"lint,test:\" --prefixColors auto",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@babel/core": "^7.27.4",
    "ember-template-imports": "^4.3.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.27.5",
    "@babel/plugin-proposal-decorators": "^7.27.1",
    "@ember/optional-features": "^2.2.0",
    "@ember/test-helpers": "^5.2.2",
    "@embroider/macros": "^1.18.0",
    "@embroider/test-setup": "^4.0.0",
    "@eslint/js": "^9.28.0",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.1.2",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "~6.5.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-deprecation-workflow": "^3.3.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-content-editable-modifier": "^3.0.0",
    "ember-load-initializers": "^3.0.1",
    "ember-page-title": "^9.0.2",
    "ember-qunit": "^9.0.3",
    "ember-resolver": "^13.1.1",
    "ember-source": "~6.5.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^6.1.0",
    "ember-try": "^4.0.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^12.5.0",
    "eslint-plugin-n": "^17.19.0",
    "eslint-plugin-qunit": "^8.1.2",
    "globals": "^15.15.0",
    "loader.js": "^4.7.0",
    "prettier": "^3.5.3",
    "prettier-plugin-ember-template-tag": "^2.0.6",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "stylelint": "^16.20.0",
    "stylelint-config-standard": "^36.0.1",
    "webpack": "^5.99.9"
  },
  "peerDependencies": {
    "ember-source": "^4.0.0 || >= 5.0.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "private": true
}
